          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH stake_by_block AS (\n    SELECT\n        b.timestamp AS time,\n        SUM(CASE WHEN n.subnet_id = 0 THEN ns.total_stake ELSE 0 END) AS root_stake,\n        SUM(CASE WHEN n.subnet_id > 0 THEN ns.total_stake ELSE 0 END) AS alpha_stake,\n        SUM(ns.total_stake) AS total_stake\n    FROM metagraph_neuron_snapshot ns\n    JOIN metagraph_neuron n ON ns.neuron_id = n.id\n    JOIN metagraph_block b ON ns.block_id = b.number\n    WHERE b.timestamp BETWEEN $__timeFrom()::timestamp AND $__timeTo()::timestamp\n    GROUP BY b.timestamp\n)\nSELECT\n    time,\n    CASE WHEN total_stake > 0 THEN (root_stake::numeric / total_stake * 100) ELSE 0 END AS \"Total Root Stake\",\n    CASE WHEN total_stake > 0 THEN (alpha_stake::numeric / total_stake * 100) ELSE 0 END AS \"Total Alpha Stake\"\nFROM stake_by_block\nORDER BY time",
          "refId": "A"
        }
      ],
      "title": "Root vs Alpha Stake Distribution",
      "type": "timeseries"
    },
